{
    "name": "attla/ulid",
    "description": "Universally Unique Lexicographically Sortable Identifier (ULID) implementation for Attla and Laravel.",
    "keywords": [
        "attla",
        "laravel",
        "ulid",
        "uuid",
        "uuid-alternative"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Nicolau",
            "email": "lucas@octha.com"
        }
    ],
    "homepage": "https://github.com/attla/ulid",
    "autoload": {
        "psr-4": {
            "Attla\\Ulid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Attla\\Ulid\\Benchmark\\": "benchmarks/",
            "Attla\\Ulid\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpbench/phpbench": "^1.1.3",
        "symfony/phpunit-bridge": "^5.1"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
